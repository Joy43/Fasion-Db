{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "typeRoots": ["./types", "./node_modules/@types"],
    "strict": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "config/app.config.js"
  ]
}
